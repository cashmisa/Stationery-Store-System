@model IEnumerable<LUSSIS.Models.Disbursement>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AcknowledgeEmployee.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CollectionPoint.CollectionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Department.DeptName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CollectionDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AcknowledgeEmployee.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CollectionPoint.CollectionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Department.DeptName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CollectionDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DisbursementId }) |
            @Html.ActionLink("Details", "Details", new { id=item.DisbursementId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DisbursementId })
        </td>
    </tr>
}

</table>
